{
    "presets": [
        "@babel/react",
        "@babel/typescript",
        ["@babel/env", { "modules": false }]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "regenerator": true
            }
        ],
        [
            "@babel/plugin-proposal-object-rest-spread"
        ]
    ]
}
